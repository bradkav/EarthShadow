Cell[TextData[{
 "You can plot some of the tabulated speed distributions in the results \
folder using the following function. Here, coeffA and coeffD multiply the \
attenuation and deflection contributions, so you can switch them on and off \
easily. You might just need to let ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " know where to find the results tables:"
}], "Text",
 CellChangeTimes->{{3.688389092956533*^9, 3.68838915899928*^9}, {
  3.688389213698043*^9, 3.688389233317643*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ResultsDir", " ", "=", " ", 
   RowBox[{
    RowBox[{"NotebookDirectory", "[", "]"}], " ", "<>", " ", 
    "\"\<../results/LowMass/\>\""}]}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{"PlotSpeedDists", "[", 
   RowBox[{"op_", ",", " ", "mx_", ",", " ", "\[Gamma]overpi_", ",", " ", 
    RowBox[{"coeffA_:", "1"}], ",", " ", 
    RowBox[{"coeffD_:", "1"}], ",", " ", 
    RowBox[{"ps_:", "0.10"}]}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Check", " ", "that", " ", "the", " ", "file", " ", "exists"}], 
    "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"mstring", " ", "=", " ", 
     RowBox[{"ToString", "[", 
      RowBox[{"NumberForm", "[", 
       RowBox[{"mx", ",", 
        RowBox[{"{", 
         RowBox[{"5", ",", "2"}], "}"}]}], "]"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"filename", " ", "=", " ", 
     RowBox[{
     "ResultsDir", " ", "<>", " ", "\"\<Speeddist_op=\>\"", " ", "<>", " ", 
      RowBox[{"ToString", "[", "op", "]"}], " ", "<>", " ", "\"\<_mx=\>\"", 
      " ", "<>", " ", "mstring", " ", "<>", " ", "\"\<_gam=\>\"", " ", "<>", 
      " ", 
      RowBox[{"ToString", "[", 
       RowBox[{"NumberForm", "[", 
        RowBox[{"\[Gamma]overpi", ",", 
         RowBox[{"{", 
          RowBox[{"5", ",", "2"}], "}"}]}], "]"}], "]"}], "<>", 
      "\"\<_ps=\>\"", "<>", " ", 
      RowBox[{"ToString", "[", 
       RowBox[{"NumberForm", "[", 
        RowBox[{"ps", ",", 
         RowBox[{"{", 
          RowBox[{"5", ",", "2"}], "}"}]}], "]"}], "]"}], "<>", " ", 
      "\"\<.dat\>\""}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"Not", "[", 
       RowBox[{"FileExistsQ", "[", "filename", "]"}], "]"}], ",", " ", 
      RowBox[{
       RowBox[{"Print", "[", 
        RowBox[{
        "\"\<Error: file '\>\"", " ", "<>", " ", "filename", " ", "<>", " ", 
         "\"\<' not found...\>\""}], "]"}], ";", "\[IndentingNewLine]", " ", 
       RowBox[{"Return", "[", 
        RowBox[{
         RowBox[{"-", "1"}], ";"}], "]"}], ";"}]}], " ", "]"}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"Load", " ", "the", " ", "data", " ", "file"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"SpeedDist", " ", "=", " ", 
     RowBox[{"Import", "[", 
      RowBox[{"filename", ",", " ", "\"\<Table\>\""}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Nv", " ", "=", 
     RowBox[{"Length", "[", 
      RowBox[{"SpeedDist", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "1"}], "]"}], "]"}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{
       RowBox[{"Calculate", " ", "free"}], " ", "+", " ", 
       RowBox[{"perturbed", " ", "distributions"}]}], ",", " ", 
      RowBox[{"and", " ", "the", " ", "ratio"}]}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"ffree", " ", "=", " ", 
     RowBox[{"Interpolation", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"SpeedDist", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "1"}], "]"}], "]"}], " ", ",", " ", 
           RowBox[{"SpeedDist", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "2"}], "]"}], "]"}]}], " ", "}"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i", ",", "Nv"}], "}"}]}], "]"}], ",", " ", 
       RowBox[{"InterpolationOrder", "\[Rule]", "1"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"fpert", " ", "=", " ", 
     RowBox[{"Interpolation", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"SpeedDist", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "1"}], "]"}], "]"}], " ", ",", " ", 
           RowBox[{
            RowBox[{"SpeedDist", "[", 
             RowBox[{"[", 
              RowBox[{"i", ",", "2"}], "]"}], "]"}], " ", "+", 
            RowBox[{"coeffA", "*", 
             RowBox[{"SpeedDist", "[", 
              RowBox[{"[", 
               RowBox[{"i", ",", "3"}], "]"}], "]"}]}], "+", 
            RowBox[{"coeffD", "*", 
             RowBox[{"SpeedDist", "[", 
              RowBox[{"[", 
               RowBox[{"i", ",", "4"}], "]"}], "]"}]}]}]}], "}"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i", ",", "Nv"}], "}"}]}], "]"}], ",", 
       RowBox[{"InterpolationOrder", "\[Rule]", "1"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"fratio", " ", "=", " ", 
     RowBox[{"Interpolation", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"SpeedDist", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "1"}], "]"}], "]"}], " ", ",", " ", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"SpeedDist", "[", 
               RowBox[{"[", 
                RowBox[{"i", ",", "2"}], "]"}], "]"}], " ", "+", " ", 
              RowBox[{"coeffA", "*", 
               RowBox[{"SpeedDist", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "3"}], "]"}], "]"}]}], "+", 
              RowBox[{"coeffD", "*", 
               RowBox[{"SpeedDist", "[", 
                RowBox[{"[", 
                 RowBox[{"i", ",", "4"}], "]"}], "]"}]}]}], ")"}], "/", 
            RowBox[{"SpeedDist", "[", 
             RowBox[{"[", 
              RowBox[{"i", ",", "2"}], "]"}], "]"}]}]}], "}"}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i", ",", "Nv"}], "}"}]}], "]"}], ",", 
       RowBox[{"InterpolationOrder", "\[Rule]", "1"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"Do", " ", "some", " ", "plotting"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{"p1", " ", "=", " ", 
     RowBox[{"Plot", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"ffree", "[", "v", "]"}], ",", " ", 
         RowBox[{"fpert", "[", "v", "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"v", ",", " ", "1", ",", " ", "753"}], "}"}], ",", " ", 
       RowBox[{"PlotTheme", "->", "\"\<Scientific\>\""}], ",", 
       RowBox[{"FrameLabel", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
         "\"\<v / km \!\(\*SuperscriptBox[\(s\), \(-1\)]\) \>\"", ",", " ", 
          "\"\<\!\(\*OverscriptBox[\(f\), \(~\)]\)(v,\[Gamma])/ \
\!\(\*SuperscriptBox[\(10\), \(-3\)]\) \!\(\*SuperscriptBox[\(km\), \(-1\)]\) \
s\>\""}], "}"}]}], ",", 
       RowBox[{"PlotLegends", "\[Rule]", 
        RowBox[{"Placed", "[", 
         RowBox[{
          RowBox[{"LineLegend", "[", 
           RowBox[{"{", 
            RowBox[{"\"\<Free\>\"", ",", " ", "\"\<Perturbed\>\""}], "}"}], 
           "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"0.75", ",", " ", "0.75"}], "}"}]}], "]"}]}], ",", 
       RowBox[{"FrameStyle", "\[Rule]", 
        RowBox[{"Directive", "[", "15", "]"}]}], ",", " ", 
       RowBox[{"ImageSize", "\[Rule]", " ", "400"}], ",", 
       RowBox[{"BaseStyle", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"FontSize", "\[Rule]", "12"}], "}"}]}], ",", " ", 
       RowBox[{"FrameStyle", "\[Rule]", "Black"}], ",", 
       RowBox[{"LabelStyle", "\[Rule]", "Black"}], ",", " ", 
       RowBox[{"PlotLabel", "\[Rule]", 
        RowBox[{
        "\"\<\!\(\*SubscriptBox[\(m\), \(\[Chi]\)]\) = \>\"", " ", "<>", " ", 
         
         RowBox[{"ToString", "[", "mx", "]"}], " ", "<>", " ", 
         "\"\< GeV;   op = \>\"", " ", "<>", " ", 
         RowBox[{"ToString", "[", "op", "]"}], " ", "<>", " ", 
         "\"\<;   \[Gamma] = \>\"", " ", "<>", " ", 
         RowBox[{"ToString", "[", "\[Gamma]overpi", "]"}], " ", "<>", " ", 
         "\"\< \[Pi] ;   \!\(\*SubscriptBox[\(p\), \(scat\)]\) = 10%\>\""}]}],
        " ", ",", " ", 
       RowBox[{"ImagePadding", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"75", ",", "10"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "10"}], "}"}]}], "}"}]}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"p2", " ", "=", " ", 
     RowBox[{"ListPlot", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"v", ",", 
           RowBox[{"fratio", "[", "v", "]"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"v", ",", 
           RowBox[{"SpeedDist", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "1"}], "]"}], "]"}]}], " ", "}"}]}], "]"}], 
       ",", " ", 
       RowBox[{"PlotTheme", "->", "\"\<Scientific\>\""}], ",", 
       RowBox[{"FrameLabel", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
         "\"\<v/ km \!\(\*SuperscriptBox[\(s\), \(-1\)]\)\>\"", ",", " ", 
          "\"\<\!\(\*OverscriptBox[\(f\), \
\(~\)]\)(v,\[Gamma])/\!\(\*SubscriptBox[\(f\), \(O\)]\)(v)\>\""}], "}"}]}], 
       ",", 
       RowBox[{"FrameStyle", "\[Rule]", 
        RowBox[{"Directive", "[", "15", "]"}]}], ",", " ", 
       RowBox[{"ImageSize", "\[Rule]", " ", "400"}], ",", " ", 
       RowBox[{"BaseStyle", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"FontSize", "\[Rule]", "12"}], "}"}]}], ",", " ", 
       RowBox[{"FrameStyle", "\[Rule]", "Black"}], ",", " ", 
       RowBox[{"PlotStyle", "\[Rule]", "Black"}], ",", 
       RowBox[{"LabelStyle", "\[Rule]", "Black"}], ",", " ", 
       RowBox[{"GridLines", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", "0", "}"}], ",", 
          RowBox[{"{", "1", "}"}]}], "}"}]}], ",", " ", 
       RowBox[{"ImagePadding", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"75", ",", "10"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"50", ",", "0"}], "}"}]}], "}"}]}], ",", " ", 
       RowBox[{"AspectRatio", "\[Rule]", "0.4"}], ",", " ", 
       RowBox[{"Joined", "\[Rule]", "True"}], ",", " ", 
       RowBox[{"InterpolationOrder", "\[Rule]", "1"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"max", "=", 
     RowBox[{"Max", "@", 
      RowBox[{"Abs", "@", 
       RowBox[{"Last", "[", 
        RowBox[{"PlotRange", "/.", 
         RowBox[{"AbsoluteOptions", "[", 
          RowBox[{"p2", ",", "PlotRange"}], "]"}]}], "]"}]}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"min", "=", 
     RowBox[{"Min", "@", 
      RowBox[{"Abs", "@", 
       RowBox[{"Last", "[", 
        RowBox[{"PlotRange", "/.", 
         RowBox[{"AbsoluteOptions", "[", 
          RowBox[{"p2", ",", "PlotRange"}], "]"}]}], "]"}]}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"diff", " ", "=", " ", 
     RowBox[{"Sqrt", "[", 
      RowBox[{"Max", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"max", "-", "1"}], ")"}], "^", "2"}], ",", " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", "min"}], ")"}], "^", "2"}]}], "]"}], "]"}]}], ";",
     "\[IndentingNewLine]", 
    RowBox[{"Column", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"p1", ",", 
        RowBox[{"Show", "[", 
         RowBox[{"p2", ",", 
          RowBox[{"PlotRange", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"0", ",", " ", "753"}], "}"}], ",", " ", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"1", "-", "diff"}], ",", " ", 
               RowBox[{"1", "+", "diff"}]}], "}"}]}], "}"}]}]}], "]"}]}], 
       "}"}], ",", " ", 
      RowBox[{"Spacings", "\[Rule]", "0"}]}], "]"}]}], "\[IndentingNewLine]", 
   ")"}]}]], "Input",
